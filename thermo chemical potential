import numpy as np

k_1 = 8.617*10**-5
T = 300
p = 1.01*10**5
k = 1.38*10**-23
h = 6.626*10**-34
m = 0.131/(6.022*10**23)
mu = k_1 * T * np.log((p/(k*T))*(h**3)/(2*np.pi*m*k*T)**(1.5))

print(mu)

print(mu)
m2 = np.exp ( (k_1*T*(np.log(p*h**3/(k*T))-1.5*np.log(2*np.pi*k*T))+0.6)/(1.5*k_1*T) )
print(m)
mu2 = k_1 * T * np.log((p/(k*T))*(h**3)/(2*np.pi*m*k*T)**(1.5)/300)
